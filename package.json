{
  "name": "live-render-prototype",
  "version": "1.0.0",
  "main": "build/src/server.js",
  "repository": "https://github.com/jacobstern/live-render-prototype",
  "author": "Jacob Stern",
  "license": "MIT",
  "scripts": {
    "assets": "parcel build \"assets/src/main.ts\" --out-dir public/assets --public-url ./",
    "assets:clean": "rimraf public/build",
    "precopy": "shx mkdir -p build/",
    "copy": "shx cp -r public/ views/ build/",
    "dev": "node scripts/dev.js",
    "ts": "tsc -p tsconfig.build.json",
    "build": "run-s assets copy ts",
    "start": "node build/src/server.js",
    "start:cross-env": "cross-env NODE_ENV=production yarn start"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "livereload": "^0.7.0",
    "prettier": "^1.17.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "yarn": "^1.16.0"
  },
  "dependencies": {
    "@types/compression": "0.0.36",
    "@types/eventsource": "1.1.0",
    "@types/express": "4.16.1",
    "@types/express-handlebars": "0.0.31",
    "@types/express-session": "1.15.12",
    "@types/getenv": "0.7.0",
    "@types/helmet": "0.0.43",
    "@types/morgan": "1.7.35",
    "@types/node": "12.0.2",
    "@types/serve-favicon": "2.2.30",
    "@types/socket.io": "2.1.2",
    "@types/socket.io-client": "1.4.32",
    "@types/uniqid": "4.1.3",
    "@types/validator": "10.11.0",
    "compression": "^1.7.4",
    "cross-env": "^6.0.3",
    "eventsource": "^1.0.7",
    "express": "^5.0.0-alpha.7",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.16.1",
    "fast-diff": "^1.2.0",
    "getenv": "^0.7.0",
    "handlebars": "^4.1.2",
    "helmet": "^3.18.0",
    "http-status-codes": "^1.3.2",
    "memorystore": "^1.6.1",
    "morgan": "^1.9.1",
    "morphdom": "^2.5.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "serve-favicon": "^2.5.0",
    "shx": "^0.3.2",
    "socket.io": "^2.2.0",
